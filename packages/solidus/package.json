{
  "name": "solidus",
  "version": "0.1.0",
  "description": "Plug and play Server-Side Rendering for SolidJS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "solidus": "./dist/cli.js"
  },
  "scripts": {
    "build": "yarn clean && rollup -c",
    "clean": "rimraf ./dist"
  },
  "license": "BSD-2",
  "devDependencies": {
    "@types/polka": "^0.5.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-preset-solid": "^1.1.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@swindle/color": "^1.0.0",
    "@swindle/core": "^2.1.1",
    "polka": "^0.5.2",
    "serve-static": "^1.14.2",
    "solid-js": "^1.3.2"
  }
}
