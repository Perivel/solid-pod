{
  "name": "solidusjs",
  "version": "0.1.0",
  "description": "Plug and play Server-Side Rendering for SolidJS",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "cli"
  ],
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.js"
      },
      "node": {
        "import": "./dist/server.js",
        "require": "./dist/server.js"
      },
      "import": "./dist/server.js",
      "require": "./dist/server.js"
